{
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@onboardbase/secure-log",
  "author": "Onboardbase",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/obblog.esm.js",
  "size-limit": [
    {
      "path": "dist/secure-log.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/secure-log.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/esbuild": "^8.2.6",
    "@size-limit/esbuild-why": "^8.2.6",
    "@size-limit/preset-small-lib": "^8.2.6",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "description": "A better and secure console logging experience.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onboardbase/secure-log.git"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/onboardbase/secure-log/issues"
  },
  "homepage": "https://github.com/onboardbase/secure-log#readme"
}
